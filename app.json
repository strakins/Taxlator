{
  "expo": {
    "owner": "strakins",
    "name": "Taxlator",
    "slug": "taxlator",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "taxlator",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.taxlator.app"
    },
    "android": {
      "package": "com.taxlator.app",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#153D8A"
      },
      "edgeToEdgeEnabled": true
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-secure-store"
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/68c614e7-bce4-4830-ab72-e9e04c09535c"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "68c614e7-bce4-4830-ab72-e9e04c09535c"
      }
    }
  }
}
